level: max
paths:
    - bin
    - config
    - public
    - src
includes:
    - vendor/phpstan/phpstan-doctrine/extension.neon
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-strict-rulesw/rules.neon
parameters:
    fileExtensions:
        - php
        - phtml
        - dist
    reportUnmatchedIgnoredErrors: true